{
  "name": "md2carddav",
  "version": "0.0.0",
  "description": "Sync between markdown + yaml and CardDAV.",
  "author": "Callum Macdonald @chmac",
  "bin": {
    "md2carddav": "./bin/run"
  },
  "homepage": "https://github.com/GenerousLabs/md2carddav",
  "license": "AGPL-3.0-or-later",
  "main": "dist/index.js",
  "repository": "GenerousLabs/md2carddav",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@covve/easy-vcard": "^1.0.0",
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "cosmiconfig": "^7.0.1",
    "fs-jetpack": "^4.3.1",
    "gray-matter": "^4.0.3",
    "postinstall-postinstall": "^2.1.0",
    "readdirp": "^3.6.0",
    "slugify": "^1.6.5",
    "tsdav": "^2.0.0-rc.2",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^2",
    "patch-package": "^6.4.7",
    "prettier": "2.5.1",
    "shx": "^0.3.3",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "oclif": {
    "bin": "md2carddav",
    "dirname": "md2carddav",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "carddav": {
        "description": "Commands related to CardDAV server contacts"
      },
      "md": {
        "description": "Commands related to local markdown contacts"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "watch": "shx rm -rf dist && tsc -b --watch",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "postinstall": "patch-package"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/GenerousLabs/md2carddav/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
